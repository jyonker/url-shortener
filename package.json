{
  "name": "url-shortener",
  "version": "0.0.1",
  "description": "A simple URL Shortener running on Heroku",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:local": "grunt clearData && $(gcloud beta emulators datastore env-init) && node app.js",
    "start:services": "gcloud beta emulators datastore start | redis-server",
    "test": "grunt ci",
    "test:integration": "$(gcloud beta emulators datastore env-init) && grunt clearData && grunt mochaTest:integration",
    "deploy": "gcloud app deploy",
    "build": "grunt build"
  },
  "dependencies": {
    "@google-cloud/datastore": "^1.1.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.14.0",
    "chance": "^0.7.7",
    "compression": "^1.6.0",
    "express": "4.13.3",
    "express-brute": "^0.6.0",
    "express-brute-redis": "0.0.1",
    "grunt": "^0.4.5",
    "load-grunt-tasks": "^3.3.0",
    "mime": "^1.3.4",
    "newrelic": "^1.25.1",
    "redis": "^2.0.1"
  },
  "engines": {
    "node": "8.9.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jyonker/url-shortener"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "expect.js": "^0.3.1",
    "grunt-banner": "^0.6.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-uglify": "^0.11.0",
    "grunt-filerev": "^2.3.1",
    "grunt-force-task": "^1.0.0",
    "grunt-mocha-test": "^0.12.7",
    "grunt-sass": "^2.0.0",
    "grunt-shell": "^1.1.2",
    "grunt-usemin": "^3.1.1",
    "mocha": "^2.3.3",
    "proxyquire": "^1.7.2",
    "sinon": "^1.17.1",
    "supertest": "^1.1.0"
  }
}
